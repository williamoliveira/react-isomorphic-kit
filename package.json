    "file-loader": "1.1.11",
    "flow-bin": "0.68.0",
    "flow-typed": "2.3.0",
    "glob": "7.1.2",
    "hard-source-webpack-plugin": "0.6.4",
    "html-webpack-plugin": "3.0.6",
    "husky": "0.14.3",
    "jest": "22.4.2",
