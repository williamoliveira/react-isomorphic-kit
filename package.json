    "rimraf": "2.6.2",
    "sass-loader": "6.0.7",
    "semver": "5.5.0",
    "source-map-support": "0.5.4",
    "style-loader": "0.20.3",
    "uglifyjs-webpack-plugin": "1.2.3",
    "webpack": "4.1.1",
    "webpack-bundle-analyzer": "2.11.1",
