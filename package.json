{
  "name": "react-universal-boilerplate",
  "version": "1.0.0",
  "description": "React Universal Boilerplate.",
  "license": "MIT",
  "main": "scripts/build.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "build": "rm -r lib; babel src --out-dir lib",
    "lint": "eslint client src",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "precommit": "lint-staged",
    "format": "prettier-eslint \"+(src)/**/*.js\" --write"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "dependencies": {
    "app-root-dir": "1.0.2",
    "chalk": "^2.3.2",
    "dotenv": "5.0.1",
    "express": "4.16.3",
    "modernizr": "3.6.0",
    "offline-plugin": "5.0.0-0",
    "prop-types": "15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-hot-loader": "4.0.0",
    "serialize-javascript": "1.4.0",
    "@babel/cli": "^7.0.0-beta.42",
    "@babel/core": "7.0.0-beta.42",
    "@babel/node": "7.0.0-beta.42",
    "@babel/preset-env": "7.0.0-beta.42",
    "@babel/register": "7.0.0-beta.42",
    "assets-webpack-plugin": "3.5.1",
